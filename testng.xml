<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Listeners">
    <listeners>
        <listener class-name="testNGLearning.TestListener"></listener>
    </listeners>
    <test verbose="2" preserve-order="true"
          name="Login Test">
        <classes>
            <class name="testNGLearning.LoginTest">
                <methods>
                    <include name="doLogin"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>